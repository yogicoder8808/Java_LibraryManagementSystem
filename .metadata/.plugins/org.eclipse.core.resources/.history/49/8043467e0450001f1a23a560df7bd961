package libraryManagement;

public class User {
    private String name;
    private String id;
    private String userNumber;

    public User(String name, String id, String userNumber) {
        this.name = name;
        this.id = id;
        this.userNumber = userNumber;
    }

    // Getters and Setters

    public String getName() {
        return name;
    }

    public String getId() {
        return id;
    }

    public String getUserNumber() {
        return userNumber;
    }
}

